@model Vidly.ViewModels.MovieViewModel
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<p>Below are out movies</p>
<table style="width:100%">
    <th>Movie Title</th>
    <tr>
        @foreach (var movie in Model.Movies)
        {
            <td>@movie.Name</td>
        }
    </tr>
</table>
