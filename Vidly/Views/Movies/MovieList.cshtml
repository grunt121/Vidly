@model Vidly.ViewModels.MovieViewModel
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<p>Below are our movies</p>

<th>Film list</th>
<tr>
  
        @foreach (var movie in Model.Movies)
        {
            <tr>
            <td>@movie.Name</td>
            </tr>
        }
 </tr>
